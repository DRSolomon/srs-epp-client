<epp xmlns="urn:ietf:params:xml:ns:epp-1.0">
  <command>
    <create>
      <contact:create xmlns:contact="urn:ietf:params:xml:ns:contact-1.0">
        <contact:id>{{ contact_handle }}</contact:id>
        <contact:postalInfo type="int">
          <contact:name>{{ contact_name }}</contact:name>
          <contact:addr>
            <contact:street>{{ contact_street1 }}</contact:street>
            {% if contact_street2 %} <contact:street>{{ contact_street2 }}</contact:street> {% endif %}
            <contact:city>{{ contact_city }}</contact:city>
            {% if contact_province %} <contact:sp>{{ contact_province }}</contact:sp> {% endif %}
            {% if contact_postcode %} <contact:pc>{{ contact_postcode }}</contact:pc> {% endif %}
            <contact:cc>{{ contact_country }}</contact:cc>
          </contact:addr>
        </contact:postalInfo>
        <contact:voice>{{ contact_phone_number }}</contact:voice>
        {% if contact_fax_number %} <contact:fax>{{ contact_fax_number }}</contact:fax> {% endif %}
        <contact:email>{{ contact_email_address }}</contact:email>
        {% if contact_disclose is defined %} <contact:disclose flag="{{ contact_disclose | int }}" /> {% endif %}
        <contact:authInfo>
          <contact:pw></contact:pw>
        </contact:authInfo>
      </contact:create>
    </create>
    <clTRID>{{ transaction_id }}</clTRID>
  </command>
</epp>
